{{!< default}}
{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}

{{!-- The big featured header, it uses blog cover image as a BG if available --}}
<header class="site-header outer jumbotron jumbotron-fluid {{#if @blog.cover_image}}" style="background-image: url({{@blog.cover_image}}){{else}}no-cover{{/if}}">
    <div class="inner">
        <div class="site-header-content">
            <h1 class="site-title display-4 serif text-center text-light">
                {{@blog.title}}
            </h1>
            <h2 class="site-description serif text-center text-light">{{@blog.description}}</h2>
        </div>
    </div>
</header>
{{!-- Homepage specific content --}}
<div class="container" id="mission-hero">
    <div class="row">
        <!--Placeholders-->
        <div class="col-sm-2"></div>
        <!--Mission Statement-->
        <div class="col-sm-8">
            {{!-- Mission statement is made uncollapsable at the moment to maintain sufficient height at all times --}}
            <h1 class="display-4 serif text-center text-dark" style="margin-bottom: 1.5rem;">
                Our mission.
                <a data-toggle="collapse" href="#mission-statement">
                    <i class="material-icons">
                        unfold_more
                    </i>
                </a>
            </h1>
            <h4 class="collapse show" id="mission-statement">
                The Imperial College Investment Society (ICIS)
                <small class="lead mission-text">
                    was founded in October 2015, with the aim of educating members on value investment techniques, and is growing exponentially
                    year on year. We host interactive weekly investment meetings where our members have the chance to present, 
					ask questions and vote on whether to add investment pitches to our virtual portfolio. By submersing 
					themselves in the worldâ€™s financial news, jargon and investment theory our members develop vital 
					commercial awareness and financial acumen; which is critical to making successful applications in 
					a competitive world. We also offer a diverse range of career opportunities ranging from Private Equity 
					to Asset Management through office visits and speaker sessions from industry professionals.
                </small>
            </h4>
            <!--Insert signature block here, if needed-->
        </div>
    </div>
</div>
{{!-- Events block --}}
<main id="events-main" class="site-main outer" style="background-image: url('assets/images/event-hero.jpg');">
    <div class="inner container">
        <h1 class="serif text-center news-updates text-dark">
            What's On
            <a data-toggle="collapse" href="#events-feed">
                <i class="material-icons">
                    unfold_more
                </i>
            </a>
        </h1>
        <div id="events-feed" class="post-feed row collapse show justify-content-center">
            {{#get "posts" include="tags" filter="primary_tag: ['event']" limit="3"}}
                {{#foreach posts}}
                    {{> "post-card"}}
                {{/foreach}}  
            {{/get}}
        </div>
        <div class="news-next-container container row justify-content-center">
            <a class="older-posts" href="./tag/event">
                <span>
                    More Events
                </span>
                <i class="material-icons align-middle">
                    arrow_forward_ios
                </i>
            </a>
        </div>
    </div>
</main>
{{!-- Newsletter block --}}
<main id="updates-main" class="site-main outer" style="background-image: url('assets/images/news-hero.jpg');">
    <div class="inner container">
        <h1 class="serif text-center news-updates text-dark">
            Announcements
            <a data-toggle="collapse" href="#update-feed">
                <i class="material-icons">
                    unfold_more
                </i>
            </a>
        </h1>
        <div id="update-feed" class="post-feed row collapse show">
            {{!-- replace "updates" for the Primary tag assigned for newsletters --}}
            {{#get "posts" include="tags" filter="primary_tag: ['updates']" limit="3"}}
                {{#foreach posts}}
                    {{> "post-card"}}
                {{/foreach}}  
            {{/get}}
        </div>
        <div class="news-next-container container row justify-content-center">
            <a class="older-posts" href="./tag/updates">
                <span>
                    More Updates
                </span>
                <i class="material-icons align-middle">
                    arrow_forward_ios
                </i>
            </a>
        </div>
    </div>
</main>
{{!-- Market block --}}
<main id="mp-updates-main" class="site-main outer" style="background-image: url('assets/images/market-hero.jpg'); background-size: cover;">
    <div class="inner container">
        <h1 class="serif text-center news-updates text-white">
            Market and Portfolio Updates
            <a data-toggle="collapse" href="#update-feed">
                <i class="material-icons">
                    unfold_more
                </i>
            </a>
        </h1>
        <div id="update-feed" class="post-feed row collapse show">
            {{!-- replace "updates" for the Primary tag assigned for newsletters --}}
            {{#get "posts" include="tags" filter="primary_tag: ['mp-updates']" limit="3"}}
                {{#foreach posts}}
                    {{> "post-card"}}
                {{/foreach}}  
            {{/get}}
        </div>
        <div class="news-next-container container row justify-content-center">
            <a class="older-posts" href="./tag/updates">
                <span>
                    More Updates
                </span>
                <i class="material-icons align-middle">
                    arrow_forward_ios
                </i>
            </a>
        </div>
    </div>
</main>
{{!-- Market block --}}
<main id="advice-main" class="site-main outer" style="background-image: url('assets/images/advice-hero.jpg'); background-size: cover; display: none;">
    <div class="inner container">
        <h1 class="serif text-center news-updates text-dark">
            Application Advice
            <a data-toggle="collapse" href="#update-feed">
                <i class="material-icons">
                    unfold_more
                </i>
            </a>
        </h1>
        <div id="update-feed" class="post-feed row collapse show">
            {{!-- replace "updates" for the Primary tag assigned for newsletters --}}
            {{#get "posts" include="tags" filter="primary_tag: ['app-advice']" limit="3"}}
                {{#foreach posts}}
                    {{> "post-card"}}
                {{/foreach}}  
            {{/get}}
        </div>
        <div class="news-next-container container row justify-content-center">
            <a class="older-posts" href="./tag/updates">
                <span>
                    More Advice
                </span>
                <i class="material-icons align-middle">
                    arrow_forward_ios
                </i>
            </a>
        </div>
    </div>
</main>
{{!-- The main content area --}}
<main id="home-main" class="site-main outer">
    <div class="inner container">
        <h1 class="serif text-center news-updates text-dark">
            Articles
            <a data-toggle="collapse" href="#home-feed">
                <i class="material-icons">
                    unfold_more
                </i>
            </a>
        </h1>
        {{!-- get filtered posts of the current page --}}
        {{#get "posts" limit=@config.posts_per_page page=pagination.page filter="tags: -['mp-updates','event','updates','partner','sponsor']" include="tags"}}
        <div id="home-feed" class="post-feed row collapse show">
            {{!-- {{#foreach posts}}
                    {{> "post-card"}}
            {{/foreach}} --}}
            {{#foreach posts}}
                {{> "post-card"}}
            {{/foreach}}
        </div>
        {{pagination}}
        {{/get}}
    </div>
</main>
